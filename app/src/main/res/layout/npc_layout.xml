<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/addIncomeSpritePic"
        android:layout_width="86dp"
        android:layout_height="111dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/add_sprite_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sprites_list" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="59dp"
        android:layout_height="61dp"
        app:layout_constraintBottom_toBottomOf="@+id/addIncomeSpritePic"
        app:layout_constraintEnd_toEndOf="@+id/addIncomeSpritePic"
        app:layout_constraintHorizontal_bias="0.407"
        app:layout_constraintStart_toStartOf="@+id/addIncomeSpritePic"
        app:layout_constraintTop_toTopOf="@+id/addIncomeSpritePic"
        app:layout_constraintVertical_bias="0.52"
        app:srcCompat="@drawable/empty" />

    <ListView
        android:id="@+id/sprites_list"
        android:layout_width="393dp"
        android:layout_height="454dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="70sp"
        android:layout_marginEnd="8dp"
        android:descendantFocusability="blocksDescendants"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>